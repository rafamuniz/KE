@using KarmicEnergy.Web.Areas.Customer.ViewModels.Tank
@using Munizoft.Extensions
@using Munizoft.MVC.Helpers
@using GridMvc.Html

@model List<ListViewModel>

@{
    Layout = "~/Views/Shared/_Internal.cshtml";
    ViewBag.Title = "Tank - Dashboard";
}
<input type="hidden" id="hfCustomerId" value="@ViewBag.CustomerId" />

<div id="tank-dashboard">
</div>

@section Scripts {
    @Scripts.Render("~/bundles/sparkline")
    <script src="~/Scripts/app/tank-dashboard.js"></script>
    <script type="text/javascript">
        $(function () {
            showLoading();
            getTanks();
            hideLoading();
        });
    </script>
}