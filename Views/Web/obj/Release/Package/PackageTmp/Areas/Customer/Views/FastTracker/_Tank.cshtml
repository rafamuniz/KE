<div class="row">
    <div class="col-sm-1 tracker-number">
        <h3>{{Count}}</h3>
    </div>

    <div class="col-sm-3 tracker-image">
        <img src="{{ImageTankModel}}">
        <p>{{MeasurementTime}} 8:44:00 am</p>
        <p>{{MeasurementDate}} 4/21/2015</p>
    </div>

    <div class="col-sm-2 tracker-text">
        <h2>{{WaterVolumePerc}}</h2>
        <div class="gals">
            <h2>{{WaterVolume}}</h2>
            <p>gals</p>
        </div>
    </div>

    <div class="col-sm-6 tracker-graph">
        <img src="~/images/graph.png">
    </div>
</div>